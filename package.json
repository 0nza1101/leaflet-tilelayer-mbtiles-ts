{
  "name": "leaflet-tilelayer-mbtiles-ts",
  "version": "1.5.8",
  "description": "Load MBTiles packages into Leaflet maps (Typescript fork)",
  "keywords": [
    "leaflet",
    "plugin",
    "tilelayer",
    "mbtiles"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Iván Sánchez Ortega <ivan@sanchezortega.es>",
  "contributors": [
    "Max Battcher <me@worldmaker.net>",
    "Jordan Nijean <contact@jordan-nijean.com>"
  ],
  "license": "MIT",
  "readme": "README.md",
  "repository": {
    "url": "https://github.com/0nza1101/leaflet-tilelayer-mbtiles-ts.git",
    "type": "git"
  },
  "homepage": "https://github.com/0nza1101/leaflet-tilelayer-mbtiles-ts",
  "bugs": {
    "url": "https://github.com/0nza1101/leaflet-tilelayer-mbtiles-ts/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "webpack serve --config ./exemple/webpack.config.js",
    "dev": "tsc -w -p .",
    "build": "tsc -p .",
    "test": "jest --colors",
    "test:watch": "jest --colors --watchAll --silent=false",
    "test:ci": "jest --ci",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prepublishOnly": "npm run build"
  },
  "perDependencies": {
    "leaflet": ">=1"
  },
  "dependencies": {
    "leaflet": "1.9.3",
    "sql.js": "0.5.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@types/jest": "29.2.5",
    "@types/leaflet": "1.9.0",
    "@types/sql.js": "0.0.29",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "cross-fetch": "3.1.5",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-jest-dom": "4.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "html-webpack-plugin": "5.5.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "jest-fetch-mock": "3.0.3",
    "prettier": "2.8.2",
    "ts-jest": "29.0.4",
    "ts-loader": "9.4.2",
    "typescript": "4.9.4",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1"
  }
}
