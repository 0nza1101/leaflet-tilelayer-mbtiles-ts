{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:jest-dom/recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "ignorePatterns": ["/dist/**", "/exemple/**/**", "/tests/bench/**", "/tests/fixtures/**", "/tests/performance/**"],
  "rules": {
    "no-console": 1,
    "prettier/prettier": 2
  }
}