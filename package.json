{
  "name": "leaflet-tilelayer-mbtiles-ts",
  "version": "1.5.7",
  "description": "Load MBTiles packages into Leaflet maps (Typescript fork)",
  "keywords": [
    "leaflet",
    "plugin",
    "tilelayer",
    "mbtiles"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Iván Sánchez Ortega <ivan@sanchezortega.es>",
  "contributors": [
    "Max Battcher <me@worldmaker.net>",
    "Jordan Nijean <contact@jordan-nijean.com>"
  ],
  "license": "MIT",
  "readme": "README.md",
  "repository": {
    "url": "https://github.com/0nza1101/leaflet-tilelayer-mbtiles-ts.git",
    "type": "git"
  },
  "homepage": "https://github.com/0nza1101/leaflet-tilelayer-mbtiles-ts",
  "bugs": {
    "url": "https://github.com/0nza1101/leaflet-tilelayer-mbtiles-ts/issues"
  },
  "scripts": {
    "start": "webpack serve  --progress --config ./exemple/webpack.config.js",
    "dev": "tsc -w -p .",
    "build": "tsc -p .",
    "test": "jest --colors",
    "test:watch": "jest --colors --watchAll --silent=false",
    "test:ci": "jest --ci",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "build:publish": "npm run build && npm publish"
  },
  "perDependencies": {
    "leaflet": ">=1"
  },
  "dependencies": {
    "sql.js": "0.5.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.9",
    "@types/jest": "26.0.20",
    "@types/leaflet": "1.5.23",
    "@types/sql.js": "0.0.29",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "cross-fetch": "3.0.6",
    "eslint": "7.20.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-jest-dom": "3.6.5",
    "eslint-plugin-prettier": "^3.3.1",
    "html-webpack-plugin": "5.2.0",
    "jest": "26.6.3",
    "jest-fetch-mock": "3.0.3",
    "leaflet": "1.7.1",
    "prettier": "2.2.1",
    "ts-jest": "26.5.1",
    "ts-loader": "8.0.17",
    "typescript": "4.1.5",
    "webpack": "5.23.0",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2"
  }
}
